<div class="form-horizontal form-label-left">
  <div class="right_col" role="main">
    <div class="page-title">
      <div class="title_left">
        <h3>Cliente ID: <%= @ventas_cliente.id %></h3>
      </div>
      <div class="title_right">
      </div>
    </div>
    <div class="clearfix"></div>
    <div class="row">
      <div class="col-md-12 col-sm-12 col-xs-12">
        <div class="x_panel">
          <div class="x_title">
            <div class="form-group pull-right">
              <%= link_to 'Editar', edit_ventas_cliente_path(@ventas_cliente), {class: 'btn btn-success'} %>
              <%= link_to 'Eliminar', @ventas_cliente, :class => 'btn btn-danger', :method => :delete, :data => {:confirm => 'EstÃ¡s Seguro?'} %>
              <%= link_to 'Regresar', ventas_clientes_path, {class: 'btn btn-primary'} %>
            </div>
            <div class="clearfix"></div>
          </div>
          <div class="x_content" style="display: block;">
            <div class="form-group">
              <label class="control-label col-md-3 col-sm-3 col-xs-12">
                Agrupador
              </label>
              <div class="col-md-6 col-sm-6 col-xs-12">
                <%= @ventas_cliente.rel_agrupador_cliente.nombre %>
              </div>
            </div>
            <div class="form-group">
              <label class="control-label col-md-3 col-sm-3 col-xs-12">
                Primer Nombre
              </label>
              <div class="col-md-6 col-sm-6 col-xs-12">
                <%= @ventas_cliente.primer_nombre %>
              </div>
            </div>
            <div class="form-group">
              <label class="control-label col-md-3 col-sm-3 col-xs-12">
                Segundo Nombre
              </label>
              <div class="col-md-6 col-sm-6 col-xs-12">
                <%= @ventas_cliente.segundo_nombre %>
              </div>
            </div>
            <div class="form-group">
              <label for="ventas_cliente_primer_apellido" class="control-label col-md-3 col-sm-3 col-xs-12">
                Primer Apellido
              </label>
              <div class="col-md-6 col-sm-6 col-xs-12">
                <%= @ventas_cliente.primer_apellido %>
              </div>
            </div>
            <div class="form-group">
              <label class="control-label col-md-3 col-sm-3 col-xs-12">
                Segundo Apellido
              </label>
              <div class="col-md-6 col-sm-6 col-xs-12">
                <%= @ventas_cliente.segundo_apellido %>
              </div>
            </div>
            <div class="form-group">
              <label for="ventas_cliente_sexo" class="control-label col-md-3 col-sm-3 col-xs-12">
                Sexo
              </label>
              <div class="col-md-6 col-sm-6 col-xs-12">
              </div>
            </div>
            <div class="form-group">
              <label class="control-label col-md-3 col-sm-3 col-xs-12">
                Cedula
              </label>
              <div class="col-md-6 col-sm-6 col-xs-12">
                <%= @ventas_cliente.cedula %>
              </div>
            </div>
            <div class="form-group">
              <label for="ventas_cliente_fecha_nacimiento" class="control-label col-md-3 col-sm-3 col-xs-12">
                Fecha Nacimiento
              </label>
              <div class="col-md-6 col-sm-6 col-xs-12">
                <%= @ventas_cliente.fecha_nacimiento %>
              </div>
            </div>
            <div class="form-group">
              <label for="ventas_cliente_email" class="control-label col-md-3 col-sm-3 col-xs-12">
                Email
              </label>
              <div class="col-md-6 col-sm-6 col-xs-12">
                <%= @ventas_cliente.email %>
              </div>
            </div>
            <div class="form-group">
              <label for="ventas_cliente_email" class="control-label col-md-3 col-sm-3 col-xs-12">
                Piso
              </label>
              <div class="col-md-6 col-sm-6 col-xs-12">
                <%= @ventas_cliente.piso %>
              </div>
            </div>
            <div class="ln_solid"></div>
          </div>
        </div>
      </div>
    </div>
  </div>
</div>
<!-- bootstrap-daterangepicker -->