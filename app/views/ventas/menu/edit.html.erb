<%= form_for @ventas_agrupador_cliente, html: {class: 'form-horizontal form-label-left'} do |f| %>
    <div class="right_col" role="main">
      <div class="">
        <div class="page-title">
          <div class="title_left">
            <h3>Editar Agrupador ID: <%= @ventas_agrupador_cliente.id %></h3>
          </div>
        </div>
        <div class="clearfix"></div>
        <div class="x_panel">
          <div class="x_title">
            <div class="form-group pull-right">
              <%= f.submit 'Guadar', {class: 'btn btn btn-success'} %>
              <%= link_to 'Cancelar', ventas_agrupador_clientes_path, {class: 'btn btn-primary'} %>
            </div>
            <div class="clearfix"></div>
          </div>
          <div class="x_content" style="display: block;">
            <%= render :partial => 'form', :locals => {:f => f} %>
          </div>
        </div>
      </div>
    </div>
<% end %>
