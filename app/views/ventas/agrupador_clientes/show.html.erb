<div class="form-horizontal form-label-left">
  <div class="right_col" role="main">
    <div class="page-title">
      <div class="title_left">
        <h3>Agrupador ID: <%= @ventas_agrupador_cliente.id %></h3>
      </div>
    </div>
    <div class="clearfix"></div>
    <div class="x_panel">
      <div class="x_title">
        <div class="form-group pull-right">
          <%= link_to 'Editar', edit_ventas_agrupador_cliente_path(@ventas_agrupador_cliente), {class: 'btn btn-success'} %>
          <%= link_to 'Eliminar', @ventas_agrupador_cliente, :class => 'btn btn-danger', :method => :delete, :data => {:confirm => 'Estás Seguro?'} %>
          <%= link_to 'Regresar', ventas_agrupador_clientes_path, {class: 'btn btn-primary'} %>
        </div>
        <div class="clearfix"></div>
      </div>
      <div class="x_content" style="display: block;">
        <div class="form-group">
          <label class="control-label col-md-3 col-sm-3 col-xs-12">
            Nombre
          </label>
          <div class="col-md-6 col-sm-6 col-xs-12">
            <%= @ventas_agrupador_cliente.nombre %>
          </div>
        </div>
        <div class="form-group">
          <label class="control-label col-md-3 col-sm-3 col-xs-12">
            Descripción
          </label>
          <div class="col-md-6 col-sm-6 col-xs-12">
            <%= @ventas_agrupador_cliente.descripcion %>
          </div>
        </div>
        <div class="ln_solid"></div>
      </div>
    </div>
  </div>
</div>
